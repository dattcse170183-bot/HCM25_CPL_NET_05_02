{
  "Logging": {
    "LogLevel": {
      "Default": "None",
      "Microsoft": "None",
      "Microsoft.Hosting.Lifetime": "None",
      "Microsoft.EntityFrameworkCore": "None"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=(local);uid=sa;pwd=12345;database=MovieTheater;TrustServerCertificate=True;"
  },
  "JwtSettings": {
    "SecretKey": "YOUR_JWT_SECRET_KEY_HERE",
    "Issuer": "MovieTheater",
    "Audience": "MovieTheaterClients",
    "ExpirationInMinutes": 60
  },
  "Authentication": {
    "Google": {
      "ClientId": "YOUR_GOOGLE_CLIENT_ID_HERE",
      "ClientSecret": "YOUR_GOOGLE_CLIENT_SECRET_HERE"
    }
  },
  "EmailSettings": {
    "SmtpServer": "smtp.gmail.com",
    "Port": 587,
    "Username": "YOUR_EMAIL_HERE",
    "Password": "YOUR_EMAIL_PASSWORD_HERE",
    "FromEmail": "YOUR_EMAIL_HERE",
    "FromName": "Movie Theater"
  },
  "VNPay": {
    "TmnCode": "YOUR_VNPAY_TMN_CODE_HERE",
    "HashSecret": "YOUR_VNPAY_HASH_SECRET_HERE",
    "BaseUrl": "https://sandbox.vnpayment.vn/paymentv2/vpcpay.html",
    "Command": "pay",
    "CurrCode": "VND",
    "Version": "2.1.0",
    "Locale": "vn",
    "ReturnUrl": "https://localhost:7201/api/Payment/vnpay-return",
    "IpnUrl": "https://localhost:7201/api/Payment/vnpay-ipn",
    "ExpiredTime": 15
  },
  "QRPayment": {
    "BankCode": "VCB",
    "AccountNumber": "YOUR_BANK_ACCOUNT_NUMBER_HERE",
    "AccountName": "Movie Theater",
    "BankName": "Vietcombank",
    "QRContent": "https://api.vietqr.io/image/VCB/YOUR_ACCOUNT_NUMBER",
    "ExpiredTime": 15,
    "PayOSClientId": "YOUR_PAYOS_CLIENT_ID_HERE",
    "PayOSApiKey": "YOUR_PAYOS_API_KEY_HERE",
    "PayOSChecksumKey": "YOUR_PAYOS_CHECKSUM_KEY_HERE",
    "PayOSReturnUrl": "https://localhost:7201/api/QRPayment/payment-success",
    "PayOSCancelUrl": "https://localhost:7201/api/QRPayment/payment-cancel"
  }
}